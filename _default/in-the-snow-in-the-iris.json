se 46s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-animation: fade-in 4s ease 46s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;<br />\n<br />\n&nbsp;&nbsp;&nbsp;&nbsp;animation-fill-mode:forwards;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-animation-fill-mode: forwards;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-o-animation-fill-mode: forwards; <br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-ms-animation-fill-mode: forwards;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-moz-animation-fill-mode: forwards; <br />\n}<br />\n.seventeen-words{ <br />\n&nbsp;&nbsp;&nbsp;&nbsp;opacity: 0;&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;animation: fade-in 4s ease 49s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-animation: fade-in 4s ease 49s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-moz-animation: fade-in 4s ease 49s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-o-animation: fade-in 4s ease 49s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-animation: fade-in 4s ease 49s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;<br />\n<br />\n&nbsp;&nbsp;&nbsp;&nbsp;animation-fill-mode:forwards;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-animation-fill-mode: forwards;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-o-animation-fill-mode: forwards; <br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-ms-animation-fill-mode: forwards;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-moz-animation-fill-mode: forwards; <br />\n}<br />\n[[/module]]<br />\n<br />\n[[module css]]<br />\n.fade-out-words{<br />\n&nbsp;&nbsp;&nbsp;&nbsp;width: 100px;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;margin: 0 auto;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;text-align: left;<br />\n}<br />\n<br />\n<br />\n@keyframes fade-in{&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;0%{ opacity: 0;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;100%{opacity:1;}<br />\n}<br />\n@-webkit-keyframes fade-in{<br />\n&nbsp;&nbsp;&nbsp;&nbsp;0%{ opacity: 0;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;100%{opacity:1;}<br />\n}<br />\n@-ms-keyframes fade-in{<br />\n&nbsp;&nbsp;&nbsp;&nbsp;0%{ opacity: 0;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;100%{opacity:1;}<br />\n}<br />\n@-o-keyframes fade-in{<br />\n&nbsp;&nbsp;&nbsp;&nbsp;0%{ opacity: 0;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;100%{opacity:1;}<br />\n}<br />\n@-moz-keyframes fade-in{<br />\n&nbsp;&nbsp;&nbsp;&nbsp;0%{ opacity: 0;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;100%{opacity:1;}<br />\n}<br />\n.first-out-words{ <br />\n&nbsp;&nbsp;&nbsp;&nbsp;opacity: 0;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-animation: fade-in 4s ease 49s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-moz-animation: fade-in 4s ease 49s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-o-animation: fade-in 4s ease 49s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-animation: fade-in 4s ease 49s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;animation-fill-mode:forwards;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-animation-fill-mode: forwards;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-o-animation-fill-mode: forwards; <br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-ms-animation-fill-mode: forwards;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-moz-animation-fill-mode: forwards; <br />\n}<br />\n.second-out-words{ <br />\n&nbsp;&nbsp;&nbsp;&nbsp;opacity: 0;&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;animation: fade-in 4s ease 7s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-animation: fade-in 4s ease 7s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-moz-animation: fade-in 4s ease 7s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-o-animation: fade-in 4s ease 7s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-animation: fade-in 4s ease 7s 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;animation-fill-mode:forwards;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-animation-fill-mode: forwards;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-o-animation-fill-mode: forwards; <br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-ms-animation-fill-mode: forwards;&nbsp;&nb