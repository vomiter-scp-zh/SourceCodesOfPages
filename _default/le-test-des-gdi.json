name=&quot;q${i}&quot; id=&quot;q${i}a${j}&quot; value=&quot;${ANSWER_VALUE_ORDER[j]}&quot; /&gt;<br />\n&lt;label for=&quot;q${i}a${j}&quot;&gt;<br />\n${ANSWERS[i][j]}<br />\n&lt;/label&gt;<br />\n&lt;/div&gt;`;<br />\n}<br />\n<br />\n}<br />\n<br />\n//fictional character question<br />\nform.innerHTML += &quot;&lt;p&gt;&quot; + QUESTIONS[QUESTIONS.length-1] + &quot;&lt;/p&gt;&quot;;<br />\n<br />\nform.innerHTML += `<br />\n&lt;div&gt;<br />\n&lt;input type=&quot;radio&quot; name=&quot;qfic&quot; id=&quot;qfic0&quot; value=&quot;0&quot; checked /&gt;<br />\n&lt;label for=&quot;qfic0&quot;&gt;<br />\n${ANSWERS[QUESTIONS.length-1][0]}<br />\n&lt;/label&gt;<br />\n&lt;/div&gt;`;<br />\n<br />\nform.innerHTML += `<br />\n&lt;div&gt;<br />\n&lt;input type=&quot;radio&quot; name=&quot;qfic&quot; id=&quot;qfic1&quot; value=&quot;1&quot; /&gt;<br />\n&lt;label for=&quot;qfic1&quot;&gt;<br />\n${ANSWERS[QUESTIONS.length-1][1]}<br />\n&lt;/label&gt;<br />\n&lt;/div&gt;`;<br />\n<br />\nform.innerHTML += '&lt;br&gt;&lt;input type=&quot;submit&quot; value=&quot;開始心靈感應分析&quot; /&gt;';<br />\n<br />\n//computing results<br />\nvar resultsOrdered = [], currentScroll;<br />\n<br />\nfunction compute() {<br />\nvar answers=[], allZeros = true;<br />\nvar fictional;<br />\n<br />\n//gathering answers<br />\nfor(var i=0; i&lt;QUESTIONS.length-1; i++) {<br />\nif(document.forms[&quot;goi&quot;][&quot;q&quot;+i].value.length == 0) {<br />\nanswers.push(0);<br />\n}<br />\nelse {<br />\nanswers.push(document.forms[&quot;goi&quot;][&quot;q&quot;+i].value);<br />\n}<br />\n<br />\nallZeros = allZeros &amp;&amp; (answers[answers.length-1] == 0);<br />\n}<br />\n<br />\nfictional = document.forms[&quot;goi&quot;][&quot;qfic&quot;].value == &quot;1&quot;;<br />\n<br />\n//creating results array for pigeonhole sort<br />\nvar results = [];<br />\n<br />\nfor(var i=0; i &lt;= MAX_DIFFERENCE; i++) {<br />\nresults.push([]);<br />\n}<br />\n<br />\n//computing results<br />\nvar diff;<br />\nresultsOrdered = [];<br />\ncurrentScroll = 5;<br />\n<br />\nfor(var i=0; i&lt;GOI_ANSWERS.length; i++) {<br />\ndiff = 0;<br />\n<br />\nfor(var j=0; j&lt;answers.length; j++) {<br />\nif(answers[j] != 0 &amp;&amp; GOI_ANSWERS[i].answers[j] != 0) {<br />\ndiff += Math.abs(answers[j] - GOI_ANSWERS[i].answers[j]);<br />\n}<br />\n}<br />\n<br />\nresults[diff].push(i);<br />\n}<br />\n<br />\n//nobody easter egg<br />\nif(allZeros) {<br />\nresultsOrdered = [NOBODY];<br />\nNOBODY.diff = 0;<br />\n}<br />\n<br />\nelse {<br />\n<br />\n//reordering results for easier iterating<br />\nfor(var i=0; i&lt;results.length; i++) {<br />\nfor(var j=0; j&lt;results[i].length; j++) {<br />\n<br />\n//this condition removes SKP and such from the closest matches for non-fictional characters<br />\nif(!GOI_ANSWERS[results[i][j]].onlyFictional || fictional || resultsOrdered.length &gt;= 4) {<br />\nGOI_ANSWERS[results[i][j]].diff = i;<br />\nresultsOrdered.push(GOI_ANSWERS[results[i][j]]);<br />\n}<br />\n<br />\n}<br />\n}<br />\n<br />\n}<br />\n<br />\n//displaying results<br />\nvar closest = document.querySelector(&quot;#goi-results #closest&quot;);<br />\nvar linkdiv = document.querySelector(&quot;#goi-results #loadmore&quot;);<br />\nvar furthest = document.querySelector(&quot;#goi-results #furthest&quot;);<br />\n<br />\nclosest.innerHTML = &quot;&lt;h3&gt;與你最匹配的關注組織是&lt;/h3&gt;&quot;<br />\n<br />\ndisplay(closest, resultsOrdered[0], resultsOrdered[0].diff);<br />\n<br />\nclosest.innerHTML += &quot;&lt;h3&gt;其他也很適合你的組織是&lt;/h3&gt;&quot;<br />\n<br />\nfor(var i=1; i&lt;5; i++) {<br />\ndisplay(closest, resultsOrdered[i], resultsOrdered[i].diff);<br />\n}<br />\n<br />\nlinkdiv.innerHTML = '&lt;a href=&quot;javascript:loadmore()&quot; id=&quot;loadmore&quot;&gt;顯示更多&lt;/a&gt;';<br />\n<br />\nfurthest.innerHTML = &quot;&lt;h3&gt;與你最背道而馳的組織是&lt;/h3&gt;&quot;<br />\n<br />\nfor(var i=2; i&gt;=0; i--) {<br />\ndisplay(furthest, resultsOrdered[resultsOrdered.length-1-i], resultsOrdered[resultsOrdered.length-1-i].diff);<br />\n}<br />\n<br />\n}<br />\n<br />\n//GoI display function<br />\nfunction display(element, goi, difference) {<br />\nvar similarity = Math.floor((MAX_DIFFERENCE - difference)/MAX_DIFFERENCE * 100);<br />\n<br />\nelement.innerHTML += `<br />\n&lt;div class=&quot;content-panel&quot;&gt;<br />\n&lt;h1&gt;${goi.name}&lt;/h1&gt;<br />\n&lt;img src=&quot;${goi.img}&quot; /&gt;<br />\n&lt;p&gt;${goi.intro}&lt;/p&gt;<br />\n&lt;p&gt;&lt;strong&gt;來源分部：&lt;/strong&gt; ${goi.branch}<br />\n&lt;p class=&quot;diff&quot;&gt;你與該組織的匹配度是${similarity}%。&lt;a href=&quot;javascript:details(${GOI_ANSWERS.indexOf(goi)})&quot;&gt;該關注組織的標準答案&lt;/a&gt;&lt;/p&gt;<br />\n&lt;h2&gt;&lt;a href=&quot;${goi.url}&quot; target=&quot;_blank&quot;&gt;閱讀更多&lt;/a&gt;&lt;/h2&gt;<br />\n&lt;div id=&quot;details${GOI_ANSWERS.indexOf(goi)}&quot;&gt;&lt;/div&gt;<br />\n&lt;/div&gt;`;<br />\n}<br />\n<br />\n//loading more GoIs<br />\nfunction loadmore() {<br />\nvar closest = document.querySelector(&quot;#goi-results #closest&quot;);<br />\n<br />\nfor(var i=0; i&lt;5 &amp;&amp; currentScroll+i &lt; resultsOrdered.length; i++) {<br />\ndisplay(closest, resultsOrdered[i+currentScroll], resultsOrdered[i+currentScroll].diff);<br />\n}<br />\n<br />\ncurrentScroll += 5;<br />\n<br />\nif(currentScroll &gt;= resultsOrdered.length) document.querySelector(&quot;#loadmore a&quot;).remove();<br />\n}<br />\n<br />\n//detailed answers of a GoI<br />\nfunction details(goi) {<br />\nvar element = document.getElementById(&quot;details&quot;+goi);<br />\nelement.innerHTML = &quot;&quot;;<br />\n<br />\nfor(var i=0; i&lt;QUESTIONS.length-1; i++) {<br />\n//jesus christ<br />\nelement.innerHTML += &quot;&lt;p&gt;&quot; + (i+1) + &quot;. &quot; + QUESTIONS[i] + &quot;&lt;br&gt;- &quot; + ANSWERS[i][ANSWER_VALUE_ORDER.indexOf(parseInt(GOI_ANSWERS[goi].answers[i]))];<br />\nelement.innerHTML += &quot;&lt;/p&gt;&quot;;<br />\n}<br />\n<br />\n}<br />\n<br />\n&lt;/script&gt;<br />\n<br />\n&lt;style&gt;<br />\nform, #goi-results, input[type=submit] {<br />\ncolor: #333;<br />\nfont-size: 14px;<br />\nfont-family: verdana;<br />\n}<br />\n<br />\ninput[type=submit] {<br />\ndisplay: block;<br />\nmargin: 0 auto;<br />\nborder: 1px solid #333;<br />\npadding: 5px;<br />\n}<br />\n<br />\nform div {<br />\nmargin-top: 3px;<br />\nmargin-left: 5px;<br />\n}<br />\n<br />\nform p {<br />\nmargin-bottom: 10px;<br />\nmargin-top: 10px;<br />\n}<br />\n<br />\n#goi-results {<br />\ntext-align: center;<br />\n}<br />\n<br />\n#goi-results img {<br />\nmax-height: 200px;<br />\nmax-width: 100%;<br />\n}<br />\n<br />\n#goi-results p {<br />\ntext-align: left;<br />\n}<br />\n<br />\n#goi-results p.diff {<br />\ntext-align: center;<br />\nfont-weight: bold;<br />\n}<br />\n<br />\n#goi-results p.diff a {<br />\nfont-weight: normal;<br />\nfont-size: 84%;<br />\n}<br />\n<br />\nh1 {<br />\ncolor: #901;<br />\nfont-weight: 700;<br />\nmargin: .7em 0 .6em;<br />\npadding: 0;<br />\n}<br />\n<br />\na {<br />\ncolor: #b01;<br />\ntext-decoration: none;<br />\n}<br />\n<br />\nh3 {<br />\ntext-transform: uppercase;<br />\nfont-size: 16px;<br />\n}<br />\n<br />\n.content-panel {<br />\nmargin: 2rem auto;<br />\npadding: 0 2rem;<br />\nborder: solid 1px #888880;<br />\nborder-radius: 10px;<br />\nbox-shadow: 3px 3px 6px #bbb;<br />\nbox-shadow: 0 2px 6px rgba(0,0,0,.5),inset 0 1px rgba(255,255,255,.3),inset 0 10px rgba(255,255,255,.2),inset 0 10px 20px rgba(255,255,255,.25),inset 0 -15px 30px rgba(0,0,0,.1);<br />\nbackground-color: #fcfdfb;<br />\n}<br />\n&lt;/style&gt;<br />\n[[/html]]\n</div>\n"