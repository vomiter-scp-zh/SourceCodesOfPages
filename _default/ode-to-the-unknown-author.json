 />\n#header h1 a span{<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: block;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;content: &quot;&quot;;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;visibility: hidden;<br />\n}<br />\n<br />\n#header h2 span {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: block;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;margin: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;padding: 19px 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;line-height: 0px;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;max-height: 0px;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;font-weight: bold;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;color: transparent;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;text-shadow: none;<br />\n}<br />\n<br />\n#header h2 span::after {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;content: &quot;為了存儲，留藏，保存。&quot;;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: block;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;margin: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;padding: 0px 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;line-height: 0px;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;max-height: 0px;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;font-weight: bold;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;color: #ffffff;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;text-shadow: 1px 1px 1px #000;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;text-shadow: 1px 1px 1px rgba(0,0,0,.8);<br />\n}<br />\n[[/module]]\n</div>\n"