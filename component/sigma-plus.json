n&nbsp;&nbsp;&nbsp;&nbsp;margin: 0 auto;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;border: none;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;padding: 0 var(--sp_area-comp-margin);<br />\n}<br />\n#footer-bar .units {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: flex;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;flex-wrap: wrap;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;gap: 0.5rem;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;justify-content: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;padding: .5rem;<br />\n}<br />\n#footer-bar .unit {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;float: none;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;width: auto;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;flex-basis: 10.25rem;<br />\n}<br />\n<br />\n/*------------------*/<br />\n/*--- MOBILE Q ---*/<br />\n/*------------------*/<br />\n<br />\n@media (max-width: 767px) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;#top-bar { padding-inline: 0; }<br />\n&nbsp;&nbsp;&nbsp;&nbsp;#top-bar div[class*=&quot;top-bar&quot;] &gt; ul { justify-content: center; }<br />\n&nbsp;&nbsp;&nbsp;&nbsp;#top-bar div[class*=&quot;top-bar&quot;] &gt; ul &gt; li { flex-grow: 1; }<br />\n}<br />\n[[/code]]<br />\n<br />\n[[/iftags]]<br />\n<br />\n[[module css]]<br />\n@import url(https://scp-zh-tr.wdfiles.com/local--code/component%3Asigma-plus/1);<br />\n[[/module]]\n</div>\n"