;&nbsp;&nbsp;&nbsp;&nbsp;display: -moz-box;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;display: -ms-flexbox;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;display: flex;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;align-item: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-webkit-box-pack: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-webkit-justify-content: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-moz-box-pack: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-ms-flex-pack: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;justify-content: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;width: 1.5em;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;height: 1.5em;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;top: -webkit-calc(1rem + var(--topbar-height-on-mobile, 0rem));<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;top: -moz-calc(1rem + var(--topbar-height-on-mobile, 0rem));<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;top: calc(1rem + var(--topbar-height-on-mobile, 0rem));<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;right: 1rem;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-webkit-border-radius: 50%;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-moz-border-radius: 50%;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;border-radius: 50%;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;font-size: 2rem;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;font-weight: 900;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;background-color: rgba(var(--white-monochrome, 255, 255, 255));<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;color: rgb(var(--swatch-primary, 51, 51, 51));<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;cursor: pointer;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;z-index: 9999;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;line-height: 1em;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;opacity: 0.5;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;text-align: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;line-height: 1.3em;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n<br />\n&nbsp;&nbsp;&nbsp;&nbsp;.image-click-fullscreen-base:active ~ .scp-image-caption,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;.image-click-fullscreen-base:hover ~ .scp-image-caption {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;position: fixed;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;bottom: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;left: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;max-width: 100vw !important;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;z-index: 1000;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;font-size: 1rem;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n}<br />\n<br />\n[[/module]]<br />\n[[div_ class=&quot;image-click-fullscreen scp-image-block [[#if {$float} | block-{$align} | nofloat ]]&quot; data-feature=&quot;image-feature&quot; style=&quot;max-width:{$width};&quot;]]<br />\n&nbsp;&nbsp;&nbsp;&nbsp;[[div_ class=&quot;image-click-fullscreen-base&quot;]]<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;[[image {$url}]]<br />\n&nbsp;&nbsp;&nbsp;&nbsp;[[/div]]<br />\n&nbsp;&nbsp;&nbsp;&nbsp;[[div_ class=&quot;image-click-fullscreen-image&quot;]]<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;[[image {$url}]]<br />\n&nbsp;&nbsp;&nbsp;&nbsp;[[/div]]<br />\n&nbsp;&nbsp;&nbsp;&nbsp;[[div_ class=&quot;scp-image-caption [[#if {$add-caption} | show | hidden ]]&quot;]]<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{$caption}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;[[/div]]<br />\n&nbsp;&nbsp;&nbsp;&nbsp;[[div_ class=&quot;mobile-exit&quot;]]<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;✖<br />\n&nbsp;&nbsp;&nbsp;&nbsp;[[/div]]<br />\n[[/div]]<br />\n[!----]<br />\n[!-- {$noise-reveal}<br />\n[[module css]]<br />\n.image-hover-noise-reveal.nofloat {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;width: {$width} !important;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;max-width: 95vw;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;min-width: initial;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;margin: 0 auto;<br />\n}<br />\n.image-hover-noise-reveal-image {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;background: {$background};<br />\n&nbsp;&nbsp;&nbsp;&nbsp;position: relative;<br />\n}<br />\n.image-hover-noise-reveal-image img.image {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;opacity: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;pointer-events: none;<br />\n}<br />\n.image-hover-noise-reveal-image iframe {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;position: absolute;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;top: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;left: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;height: 100%;<br />\n}<br />\n[[/module]]<br />\n[[div_ class=&quot;image-hover-noise-reveal scp-image-block [[#if {$float} | block-{$align} | nofloat ]]&quot; data-feature=&quot;image-feature&quot; style=&quot;max-width:{$width};&quot;]]<br />\n[[div_ class=&quot;image-hover-noise-reveal-image&quot; style=&quot;max-width:{$width};&quot;]]<br />\n[[image {$url}]]<br />\n[[html class=&quot;image&quot;]]<br />\n&lt;head&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&lt;style&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;.svg-container {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;position: relative;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;.svg-container svg {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;position: absolute;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;width: 100% !important;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;top: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;left: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-o-object-fit: contain;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;object-fit: contain;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;.svg-container img {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;pointer-events: none;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;height: 100%;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;opacity: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&lt;/style&gt;<br />\n&lt;/head&gt;<br />\n&lt;body&gt;<br />\n&lt;div class=&quot;svg-container&quot; style=&quot;max-width:{$width};&quot;&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&lt;svg preserveAspectRatio=&quot;xMidYMin meet&quot;&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;defs&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;filter id=&quot;feDisplacementMap&quot;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;filterUnits=&quot;userSpaceOnUse&quot; x=&quot;0&quot; y=&quot;0&quot; width=&quot;100%&quot; height=&quot;100%&quot;&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;feTurbulence type=&quot;fractalNoise&quot; baseFrequency=&quot;0.{$fractal-size}&quot; numOctaves=&quot;1&quot; seed=&quot;1&quot; stitchTiles=&quot;noStitch&quot; result=&quot;img&quot;/&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;feDisplacementMap in=&quot;SourceGraphic&quot; in2=&quot;img&quot; xChannelSelector=&quot;R&quot; yChannelSelector=&quot;G&quot; scale=&quot;600&quot;&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;animate attributeName=&quot;scale&quot;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;values=&quot;600;0;0&quot; keyTimes=&quot;0;0.75;1&quot; keySpines=&quot;0.4, 0.0, 0.2, 1&quot; begin=&quot;mask.mouseover&quot; dur=&quot;{$speed}ms&quot; fill=&quot;freeze&quot;/&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;animate attributeName=&quot;scale&quot;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;values=&quot;0;600;600&quot; keyTimes=&quot;0;0.75;1&quot; keySpines=&quot;0.4, 0.0, 0.2, 1&quot; begin=&quot;mask.mouseout&quot; dur=&quot;{$speed}ms&quot; fill=&quot;freeze&quot;/&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;/feDisplacementMap&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;/filter&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;/defs&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;g filter=&quot;url(#feDisplacementMap)&quot;&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;image xlink:href=&quot;{$url}&quot; width=&quot;100%&quot; height=&quot;100%&quot;/&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;/g&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;rect id=&quot;mask&quot; x=&quot;0px&quot; y=&quot;0px&quot; width=&quot;100%&quot; height=&quot;100%&quot; opacity=&quot;0&quot;/&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&lt;/svg&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&lt;img id=&quot;imageNoiseBase&quot; width=&quot;100%&quot; style=&quot;max-width:{$width};&quot; src=&quot;{$url}&quot; /&gt;<br />\n&lt;/div&gt;<br />\n&lt;script&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const inputURL = &quot;{$url}&quot;;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const baseImage = document.getElementById(&quot;imageNoiseBase&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const svgImageBase = document.querySelectorAll(&quot;svg image&quot;)[0];<br />\n&nbsp;&nbsp;&nbsp;&nbsp;try {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let testURL;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;testURL = new URL(inputURL);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;} catch (_) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var imageURL = new URL(&quot;{$url}&quot;, /.*(local\\-\\-html\\/([^\\/]+)\\/)/g.exec(document.location)[0].replace(/local\\-\\-html/i, 'local--files')).href;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;baseImage.src = imageURL;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;svgImageBase.setAttribute(&quot;xlink:href&quot;, `${imageURL}`);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;if (location.protocol === 'https:') {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;baseImage.src = baseImage.src.replace(/^http:\\/\\//i, 'https://');<br />\n&nbsp;&nbsp;&nbsp;&nbsp;} else {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;baseImage.src = baseImage.src.replace(/^https:\\/\\//i, 'http://');<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;scaleSVG = () =&gt; {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;const svgImage = document.getElementsByTagName(&quot;svg&quot;)[0];<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;const imageNoiseBase = document.getElementById(&quot;imageNoiseBase&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;const imageRatio = imageNoiseBase.height / imageNoiseBase.width;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;const svgImageWidthBase = &quot;{$width}&quot;;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let svgImageWidth = parseFloat(svgImageWidthBase.replace(/[^-\\d\\.]/g, ''));<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let svgImageWidthUnits = svgImageWidthBase.replace(/[0-9]|\\./g, &quot;&quot;)<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let svgImageHeight = svgImageWidth * imageRatio;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if(svgImageWidthUnits === &quot;rem&quot;) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;svgImageWidth = svgImageWidth * 16;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;svgImageHeight = svgImageHeight * 16;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;svgImageWidthUnits = &quot;px&quot;;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;};<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if(svgImageWidthUnits === &quot;em&quot;) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let emSize = parseFloat(getComputedStyle(parentElement).fontSize);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;svgImageWidth = svgImageWidth * emSize;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;svgImageHeight = svgImageHeight * emSize;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;svgImageWidthUnits = &quot;px&quot;;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;};<br />\n&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;svgImage.setAttribute(&quot;width&quot;, svgImageWidth + svgImageWidthUnits);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;svgImage.setAttribute(&quot;height&quot;, svgImageHeight + svgImageWidthUnits);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var svgViewport = `0 0 ' ${svgImageWidth} ${svgImageHeight}`;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;svgImage.setAttribute(&quot;viewBox&quot;, svgViewport);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;};<br />\n&nbsp;&nbsp;&nbsp;&nbsp;baseImage.src = baseImage.src + &quot;?_=&quot;+(new Date().getTime());<br />\n&nbsp;&nbsp;&nbsp;&nbsp;baseImage.onload = () =&gt; {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;scaleSVG();<br />\n&nbsp;&nbsp;&nbsp;&nbsp;};<br />\n&lt;/script&gt;<br />\n&lt;/body&gt;<br />\n[[/html]]<br />\n[[/div]]<br />\n[[div_ class=&quot;scp-image-caption [[#if {$add-caption} | show | hidden ]]&quot;]]<br />\n&nbsp;&nbsp;&nbsp;&nbsp;{$caption}<br />\n[[/div]]<br />\n[[/div]]<br />\n[!----]<br />\n[!-- {$pixel-reveal}<br />\n[[module css]]<br />\n.image-pixelate-reveal.nofloat {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;width: {$width} !important;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;max-width: 95vw;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;min-width: initial;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;margin: 0 auto;<br />\n}<br />\n.image-pixelate-reveal-image {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;position: relative;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;overflow: hidden;<br />\n}<br />\n.image-pixelate-reveal-image img.image {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;opacity: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;visibility: hidden;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;pointer-events: none;<br />\n}<br />\n.image-pixelate-reveal-image iframe {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;position: absolute;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;top: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;left: 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;width: 100%;<br />\n}<br />\n[[/module]]<br />\n[[div_ class=&quot;image-pixelate-reveal scp-image-block [[#if {$float} | block-{$align} | nofloat ]]&quot; data-feature=&quot;image-feature&quot; style=&quot;max-width:{$width};&quot;]]<br />\n[[div_ class=&quot;image-pixelate-reveal-image&quot;]]<br />\n[[image {$url}]]<br />\n[[html class=&quot;image&quot; style=&quot;max-width: {$width};&quot;]]<br />\n&lt;style&gt;<br />\n.hidden {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: none;<br />\n}<br />\n#imagePxlBase {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;width: 100%;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;max-width: {$width};<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-o-object-fit: contain;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;object-fit: contain;<br />\n}<br />\n&lt;/style&gt;<br />\n&lt;div id=&quot;imagePxlParent&quot;&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;img id=&quot;imagePxlBase&quot; width=&quot;{$width}&quot; src=&quot;{$url}&quot; /&gt;<br />\n&lt;/div&gt;<br />\n&lt;script src=&quot;https://scp-wiki.wdfiles.com/local--files/component%3Aimage-features-source/modernizr-custom.js&quot;&gt;&lt;/script&gt;<br />\n&lt;script src=&quot;https://scp-wiki.wdfiles.com/local--files/component%3Aimage-features-source/pixel-flow.cjs.js&quot;&gt;&lt;/script&gt;<br />\n&lt;script&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const inputURL = &quot;{$url}&quot;;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const baseImage = document.getElementById(&quot;imagePxlBase&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;try {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let testURL;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;testURL = new URL(inputURL);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;} catch (_) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var imageURL = new URL(&quot;{$url}&quot;, /.*(local\\-\\-html\\/([^\\/]+)\\/)/g.exec(document.location)[0].replace(/local\\-\\-html/i, 'local--files')).href;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;baseImage.src = imageURL;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;if (location.protocol === 'https:') {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;baseImage.src = baseImage.src.replace(/^http:\\/\\//i, 'https://');<br />\n&nbsp;&nbsp;&nbsp;&nbsp;} else {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;baseImage.src = baseImage.src.replace(/^https:\\/\\//i, 'http://');<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;createPixelImage = () =&gt; {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;const imagePxlParent = document.getElementById(&quot;imagePxlParent&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;const imagePxl = document.getElementById(&quot;imagePxlBase&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;const imageRatio = imagePxl.width/imagePxl.height;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if (Modernizr.canvas) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let pixelImage = imagePxl;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let pixelFlows = new PixelFlow(pixelImage, { resolution: {$pixel-res} });<br />\n<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;imagePxlParent.addEventListener(&quot;mouseenter&quot;, function(){&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;pixelFlows.simpleanimate(0, {$speed});<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;});<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;imagePxlParent.addEventListener(&quot;mouseleave&quot;, function(){&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;pixelFlows.simpleanimate({$pixel-res}, {$s