n&lt;/style&gt;<br />\n&lt;script src=&quot;https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js&quot; integrity=&quot;sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==&quot; crossorigin=&quot;anonymous&quot;&gt;&lt;/script&gt;<br />\n&lt;/head&gt;<br />\n&lt;body&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&lt;div class=&quot;revealer&quot; id=&quot;sliderImageParent&quot; width=&quot;{$width}&quot; height=&quot;&quot;&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;div class=&quot;img&quot; id=&quot;top-img&quot;&gt;&lt;/div&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;div class=&quot;img&quot; id=&quot;btm-img&quot;&gt;&lt;/div&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;input type=&quot;range&quot; min=&quot;0&quot; max=&quot;99&quot; value=&quot;{$slider-start-percent}&quot; class=&quot;slider&quot; name=&quot;slider&quot; id=&quot;slider&quot;&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;div id=&quot;slider-button&quot;&gt;&lt;/div&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&lt;img id=&quot;sliderImageBase&quot; width=&quot;100%&quot; style=&quot;max-width:{$width};&quot; src=&quot;{$left-url}&quot; /&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&lt;/div&gt;<br />\n<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&lt;script&gt;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const inputLeftURL = &quot;{$left-url}&quot;;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const inputRightURL = &quot;{$right-url}&quot;;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const topImg = document.getElementById(&quot;top-img&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const btmImg = document.getElementById(&quot;btm-img&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const baseImage = document.getElementById(&quot;sliderImageBase&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const sliderButton = document.getElementById(&quot;slider-button&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const sliderArrowBG = window.getComputedStyle(sliderButton).backgroundColor.replace(/[a-z]|\\(|\\)/g, &quot;&quot;).replace(/[^0-9|\\s]/g, &quot;&quot;).split(' ');<br />\n&nbsp;&nbsp;&nbsp;&nbsp;let sliderArrowColor;<br />\n<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const testValidURL = (inputURL) =&gt; {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let validURL;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;try {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;validURL = new URL(inputURL);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;} catch (_) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let validURL = new URL(inputURL, /.*(local\\-\\-html\\/([^\\/]+)\\/)/g.exec(document.location)[0].replace(/local\\-\\-html/i, &quot;local--files&quot;)).href;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;inputURL = validURL + &quot;?_=&quot;+(new Date().getTime());<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if (location.protocol === &quot;https:&quot;) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;inputURL = inputURL.replace(/^http:\\/\\//i, &quot;https://&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;} else {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;inputURL = inputURL.replace(/^https:\\/\\//i, &quot;http://&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;return inputURL;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const setBackgroundSize = (elem, size) =&gt; {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;$(elem).css(&quot;-webkit-background-size&quot;, size);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;$(elem).css(&quot;-moz-background-size&quot;, size);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;$(elem).css(&quot;-o-background-size&quot;, size);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;$(elem).css(&quot;background-size&quot;, size);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const resizeImage = () =&gt; {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let baseImageParent = document.getElementById(&quot;sliderImageParent&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let imageWidth = baseImage.width;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let imageHeight = baseImage.height;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;sliderArrowColor = setSliderArrowColor(sliderArrowBG);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;baseImageParent.setAttribute(&quot;width&quot;, `${imageWidth}px`);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;baseImageParent.setAttribute(&quot;height&quot;, `${imageHeight}px`);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;setBackgroundSize(topImg, `${imageWidth}px 100%`);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;setBackgroundSize(btmImg, `${imageWidth}px 100%`);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n<br />\n&nbsp;&nbsp;&nbsp;&nbsp;const insertResizeImage = () =&gt; {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;$(topImg).css(&quot;background-image&quot;, `url(&quot;${testValidURL(inputLeftURL)}&quot;)`);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;$(btmImg).css(&quot;background-image&quot;, `url(&quot;${testValidURL(inputRightURL)}&quot;)`);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;resizeImage();<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n<br />\n&nbsp;&nbsp;&nbsp;&nbsp;let setSliderArrowColor = (color) =&gt; {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;let sum = Math.round(((parseInt(color[0]) * 299) + (parseInt(color[1]) * 587) + (parseInt(color[2]) * 114)) / 1000);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;console.log(&quot;color: &quot; + color);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;console.log(&quot;sum: &quot; + sum);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;return (sum &gt; 128) ? &quot;black&quot; : &quot;white&quot;;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;$(function() {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;baseImage.src = testValidURL(inputLeftURL);<br />\n<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;baseImage.onload = () =&gt; {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;sliderArrowColor = setSliderArrowColor(sliderArrowBG);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;console.log(&quot;sliderArrowColor: &quot; + sliderArrowColor);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;sliderButton.style.setProperty(&quot;--button-arrow-color&quot;, `${sliderArrowColor}`);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;window.addEventListener(&quot;resize&quot;, resizeImage);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;insertResizeImage();<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;$(&quot;#slider&quot;).on(&quot;input change&quot;, (e)=&gt;{<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;const sliderPos = e.target.value;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;$(&quot;#top-img&quot;).css(&quot;width&quot;, `${sliderPos}%`)<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;sliderButton.setAttribute(&quot;style&quot;, `<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;--button-arrow-color: ${sliderArrowColor};<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;left: -webkit-calc((${sliderPos}% - ({$slider-circle-size} * 0.2)) - ({$slider-circle-size} * 0.2));<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;left: -moz-calc((${sliderPos}% - ({$slider-circle-size} * 0.2)) - ({$slider-circle-size} * 0.2));<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;left: calc((${sliderPos}% - ({$slider-circle-size} * 0.2)) - ({$slider-circle-size} * 0.2));<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;`);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;});<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;});<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&lt;/script&gt;<br />\n&lt;/body&gt;<br />\n[[/html]]<br />\n[[image {$left-url}]]<br />\n[[/div]]<br />\n[[div_ class=&quot;scp-image-caption [[#if {$add-caption} | show | hidden ]]&quot;]]<br />\n&nbsp;&nbsp;&nbsp;&nbsp;{$caption}<br />\n[[/div]]<br />\n[[/div]]<br />\n[!----]\n</div>\n"