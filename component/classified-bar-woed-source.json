"<h1>頁面源代碼</h1>\n\n<div class=\"page-source\">\n\t[[module CSS]]<br />\n@import url(&quot;https://use.typekit.net/inx2jjr.css&quot;);<br />\n<br />\ndiv.scale {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;/* Classification Colors */<br />\n&nbsp;&nbsp;&nbsp;&nbsp;--woedbar-keter-color: rgb(180, 30, 35);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;--woedbar-euclid-color: rgb(225, 205, 35);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;--woedbar-safe-color: rgb(35, 145, 70);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;--woedbar-thaumiel-color: rgb(40, 60, 150);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;--woedbar-neutral-color: rgb(127, 127, 127);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;/* Angled Bar Colors */<br />\n&nbsp;&nbsp;&nbsp;&nbsp;--woedbar-lvl1-color: rgba(var(--swatch-menubg-black-color,12, 12, 12),0.1);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;--woedbar-lvl2-color: rgba(var(--swatch-menubg-black-color,12, 12, 12),0.3);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;--woedbar-lvl3-color: rgba(var(--swatch-menubg-black-color,12, 12, 12),0.55);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;--woedbar-lvl4-color: rgba(var(--swatch-menubg-black-color,12, 12, 12),0.75);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;--woedbar-lvl5-color: rgba(var(--swatch-menubg-black-color,12, 12, 12),1);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;--woedbar-lvl6-color: rgba(var(--swatch-primary-bright,140, 25, 26),1);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;/* Class Bar Color */<br />\n&nbsp;&nbsp;&nbsp;&nbsp;--woedbar-class-bar-color: rgb(var(--swatch-menubg-black-color,12, 12, 12));<br />\n}<br />\n<br />\ndiv.scale {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;width: -moz-fit-content;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;width: -webkit-fit-content;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;width: fit-content;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;max-width: 98vw;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;margin: 0 auto;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;padding: 1em 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;border: none;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: -ms-grid;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: grid;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;grid-template-areas:<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&quot;class classimage item&quot;;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-grid-columns: 1.25fr auto 2.5fr;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;grid-template-columns: 1.25fr auto 2.5fr;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-grid-rows: minmax(3em,5em);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;grid-template-rows: minmax(3em,5em);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-box-align: stretch;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-align-items: stretch;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-moz-box-align: stretch;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-ms-flex-align: stretch;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;align-items: stretch;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-box-pack: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-justify-content: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-moz-box-pack: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-ms-flex-pack: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;justify-content: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;font-size: clamp(0.4845rem, 0.1609rem + 1.6179vw, 0.9375rem);<br />\n}<br />\n<br />\ndiv.class1 {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-grid-row: 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-grid-column: 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;grid-area: class;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;white-space: nowrap;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;padding-right: 1em;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: -ms-grid;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: grid;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-box-pack: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-justify-content: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-moz-box-pack: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-ms-flex-pack: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;justify-content: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nb