p;&nbsp;transparent 75%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 75%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 100%);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;background-image:<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-moz-linear-gradient(315deg,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;transparent 25%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 25%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 50%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;transparent 50%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;transparent 75%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 75%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 100%);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;background-image:<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-o-linear-gradient(315deg,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;transparent 25%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 25%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 50%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;transparent 50%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;transparent 75%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 75%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 100%);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;background-image:<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;linear-gradient(135deg,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;transparent 25%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 25%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 50%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;transparent 50%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;transparent 75%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 75%,<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;var(--woedbar-lvl2-color) 100%);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-background-size: 5px 5px;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-moz-background-size: 5px 5px;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nb