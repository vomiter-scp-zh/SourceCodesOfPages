e(2) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: none;<br />\n}<br />\n<br />\ndiv.class1 .false:nth-of-type(1) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: none !important;<br />\n}<br />\n<br />\ndiv.class1 .false:nth-of-type(2) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: inline !important;<br />\n}<br />\n<br />\ndiv.level-text {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-grid-row: 2;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-grid-column: 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;grid-area: leveltxt;<br />\n}<br />\n<br />\ndiv.class-text {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-grid-row: 3;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-grid-column: 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;grid-area: classtxt;<br />\n}<br />\n<br />\ndiv.class1image {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-grid-row: 1;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-ms-grid-column: 2;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: -webkit-box;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: -webkit-flex;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: -moz-box;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: -ms-flexbox;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;display: flex;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;grid-area: classimage;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-box-align: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-align-items: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-moz-box-align: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;-ms-flex-align: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;align-items: center;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;overflow: hidden;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;margin: 0 0.6em 0 0;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;padding-right: 0.6em;<br />\n}<br />\n<br />\n/* middle images */<br />\ndiv.classified-bar {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;position: relative;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;-webkit-mask-image: url(&quot;data:image/svg+xml,%3C%3Fxml version='1.0' encoding='utf-8'%3F%3E%3Csvg version='1.2' baseProfile='tiny' id='classified-bar' xmlns='http://www.w3.org/2000/svg' xmlns:xlin