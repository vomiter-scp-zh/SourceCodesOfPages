p;&nbsp;&nbsp;var bReload= true;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;if ( ( ua.indexOf(&quot;applewebkit&quot;) &gt;= 0 ||&nbsp;&nbsp;ua.indexOf(&quot;firefox&quot;) &gt;= 0 ) &amp;&amp; ua.indexOf(&quot;edge&quot;) == -1 )<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;bReload= false;<br />\n<br />\n&nbsp;&nbsp;&nbsp;&nbsp;$('.fader').click(closeCredit);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;function closeCredit() {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if ( bReload ) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;history.go(-2);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;parent.location.href = document.referrer.split('#')[0];<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;} else {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;history.go(-2);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n});<br />\n&lt;/script&gt;<br />\n&lt;style&gt;<br />\n.fader {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;width: 100%;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;height: 100%;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;position: fixed;<br />\n}<br />\n&lt;/style&gt;<br />\n&lt;/head&gt;<br />\n&lt;body&gt;<br />\n&lt;div class='fader'&gt;&lt;/div&gt;<br />\n&lt;/body&gt;<br />\n&lt;/html&gt;<br />\n[[/code]]\n</div>\n"