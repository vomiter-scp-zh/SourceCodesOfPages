lick&quot;,closeCredit);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;function closeCredit() {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;if ( bReload ) {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;history.go(-1);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;parent.location.href = document.referrer.split('#')[0];<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;} else {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;history.go(-1);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;$('.fader').off(&quot;click&quot;);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;setTimeout(function(){<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;$('.fader').on(&quot;click&quot;,closeCredit);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;},1200);<br />\n&nbsp;&nbsp;&nbsp;&nbsp;}<br />\n});<br />\n&lt;/script&gt;<br />\n&lt;style&gt;<br />\n.fader {<br />\n&nbsp;&nbsp;&nbsp;&nbsp;width: 100%;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;height: 100%;<br />\n&nbsp;&nbsp;&nbsp;&nbsp;position: fixed;<br />\n}<br />\n&lt;/style&gt;<br />\n&lt;/head&gt;<br />\n&lt;body&gt;<br />\n&lt;div class='fader'&gt;&lt;/div&gt;<br />\n&lt;/body&gt;<br />\n&lt;/html&gt;<br />\n[[/code]]<br />\n<br />\n**End**<br />\n[[code type=&quot;html&quot;]]<br />\n&lt;!DOCTYPE html PUBLIC &quot;-//W3C//DTD XHTML 1.0 Transitional//EN&quot; &quot;http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd&quot;&gt;<br />\n&lt;html xmlns=&quot;http://www.w3.org/1999/xhtml&quot; xml:lang=&quot;ja&quot; lang=&quot;ja&quot;&gt;<br />\n&lt;head&gt;<br />\n&lt;meta http-equiv=&quot;Content-Script-Type&quot; content=&quot;text/javascript&quot;/&gt;<br />\n&lt;meta http-equiv=&quot;Content-Style-Type&quot; content=&quot;text/css&quot;/&gt;<br />\n&lt;meta http-equiv=&quot;content-language&quot; content=&quot;ja,jp-ja&quot;/&gt;<br />\n&lt;meta charset=&quot;UTF-8&quot;/